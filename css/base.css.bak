body{
	font:12px "微软雅黑",Arial;
	color:#666;
	margin:0;
	padding:0;
	background-color:#f5f5f5;
}
p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dd{
	margin:0;
	padding:0;
	list-style:none;
}
a{
	text-decoration:none;
	color:#666;
}
a:hover{
	color:#e60012;
}
img{
	vertical-align:middle;
	border:0;
}
.lf{
	float:left;
}
.rf{
	float:right;
}
.clear:after{
	content:"";
	display:block;
	clear:both;
}
div.main{
	width:1100px;
	min-width:1000px;
	margin:0 auto;
}
/*引入头部*/
#header{
	width:100%;
	height:100px;
}
/*引入底部*/
#footer{
	width:100%;
	height:424px;
}