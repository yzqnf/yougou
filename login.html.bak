		<!-- 用户注册 -->
		<div class="yg_cen_box2">
			<ul class="register_tab">
				<li class="tab_sig">
					<a href="">登录</a>
				</li>
				<li class="tab_reg">
					<a href="">注册</a>
				</li>
			</ul>
			<div style="margin-left:32px;">
				<dl class="reg_mobile">
					<dt>
						<div>
							<span>手机号</span>
							<i></i>
							<input type="text" placeholder="请输入你的手机号" maxlength="11" value="">
						</div>
					</dt>
					<dd>
						<div id="reg_mobile_tip" class="errortips">请输入手机号码</div>
					</dd>
				</dl>
				<dl class="reg_vCode">
					<dt>
						<div>
							<span>验证码</span>
							<input type="text" placeholder="请输入验证码" maxlength="11" value="">
						</div>
						<div id="vcode">
							<img src="http://www.yougou.com" alt="">
						</div>
					</dt>
					<dd>
						<div id="reg_code_tip" class="errortips">请输入验证码</div>
					</dd>
				</dl>
				<dl class="reg_mobileCode">
					<dt>
						<div>
							<span>短信验证码</span>
							<input type="text" placeholder="请输入短信验证码" maxlength="11" value="">
						</div>
						<div id="getMobileCode">
							<a href="">
								<span>获取验证码</span>
							</a>
						</div>
					</dt>
					<dd>
						<div id="reg_code2_tip" class="errortips">请输入短信验证码</div>
					</dd>
				</dl>
				<dl class="reg_password1">
					<dt>
						<div>
							<span>密码</span>
							<input type="text" placeholder="字母、数字、符号均可，6-25个字符以内" maxlength="25" value="">
						</div>
					</dt>
					<dd>
						<div id="reg_password_tip1" class="errortips">请输入密码</div>
					</dd>
				</dl>
				<dl class="reg_password2">
					<dt>
						<div>
							<span>确认密码</span>
							<input type="text" maxlength="25" value="">
						</div>
					</dt>
					<dd>
						<div id="reg_password_tip2" class="errortips">请输入确认密码</div>
					</dd>
				</dl>
				<p class="reg_rules">
           <span> 
						<input type="checkbox" id="rules" name="rules" style="margin-top:0" checked="checked"> 
					 </span>
           <label for="rules">我已阅读并同意
						 <a href="http://www.yougou.com/help/registrationagreement.html" class="Red undline" target="_blank">《优购会员注册协议》
						 </a>
